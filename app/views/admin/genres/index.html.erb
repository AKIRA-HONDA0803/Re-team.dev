<h1>ジャンル一覧・追加</h1>
<%= form_with model:@genre, url:'/admin/genres', local:true do |f| %>
   <%= f.label :ジャンル %>
   <%= f.text_field :name %>
   <%= f.submit '新規登録' %>
<% end %>

<h1>ジャンル</h1>
<% @genres.each do |genre| %>
<%= genre.name %>
<%= link_to "編集する", edit_admin_genre_path(genre)%>
<% end %>
